func distance2(x: int, y: int) -> int {
    var x2: int = x * x;
    var y2: int = y * y;
    return x2 + y2;
}

class Point {
    public var x: int;
    public var y: int;

    public func add() -> int {
        return x + y;
    }
}

func adderProducer(c: int) -> func(int, int) -> int {
    return func(a: int, b: int) -> int {
        return a + b + c;
    };
}

func makePoint() -> Point {
    return new Point();
}

func main() -> void {
    var pt: Point = new Point();
    pt.x = 10;
    pt.y = 20;
    var pt2: Point = makePoint();
    pt2.x = pt.x;
    pt2.y = pt.y;
}
